<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <servlet>
  	<servlet-name>donation</servlet-name>
  	<servlet-class>com.arunsh.BloodDonationOne</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>donation</servlet-name>
  	<url-pattern>/blood</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>log</servlet-name>
  	<servlet-class>com.arunsh.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>log</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>request</servlet-name>
  	<servlet-class>com.arunsh.BloodDonationTwo</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>request</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>cre</servlet-name>
  	<servlet-class>com.arunsh.Create</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>cre</servlet-name>
  	<url-pattern>/create</url-pattern>
  </servlet-mapping>
</web-app>